class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0)
				{
					return false;
				}
         long long int c, s=0,r;
		    c=x;
		    while(x>0)
		    
		    {
		        r=x%10;
		        s=r+(s*10);
		        x=x/10;
		    }
		    if (c==s)
		    return true;
		    else
		  return false ;
    
		}
};
